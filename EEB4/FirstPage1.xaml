                                                    <LinearColorKeyFrame KeyTime="{ThemeResource ScrollBarContractDelay}" Value="{ThemeResource ScrollBarThumbBackgroundColor}" />
                                                    <LinearColorKeyFrame KeyTime="{ThemeResource ScrollBarContractFinalKeyframe}" Value="{ThemeResource ScrollBarPanningThumbBackgroundColor}" />
                                                </ColorAnimationUsingKeyFrames>
                                                <ColorAnimationUsingKeyFrames Storyboard.TargetName="VerticalThumb"
                                                                          Storyboard.TargetProperty="(Panel.Background).(SolidColorBrush.Color)">
                                                    <LinearColorKeyFrame KeyTime="{ThemeResource ScrollBarContractDuration}" Value="{ThemeResource ScrollBarThumbBackgroundColor}" />
                                                    <LinearColorKeyFrame KeyTime="{ThemeResource ScrollBarContractDelay}" Value="{ThemeResource ScrollBarThumbBackgroundColor}" />
